  "version": "1.0",
  "publisher": "akhilharihar",
  "author": {
    "name": "Akhil Harihar"
  },
  "icon": "assets/images/icon.png",
  "bugs": {
    "url": "https://github.com/akhilharihar/vscode-instant-terminal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akhilharihar/vscode-instant-terminal"
  },
  "engines": {
